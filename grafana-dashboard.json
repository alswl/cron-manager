              "__name__": true,
              "instance": true,
              "job": true
            },
            "indexByName": {
              "Time": 0,
              "Value #Duration": 4,
              "Value #ExitCode": 5,
              "Value #LastRun": 6,
              "Value #Running": 3,
              "Value #Status": 2,
              "name": 1
            },
            "renameByName": {
